{
    "name": "damarev/filament-location-field",
    "description": "This is my package filament-location-field",
    "keywords": [
        "damarev",
        "laravel",
        "filamentphp",
        "filament-location-field"
    ],
    "version": "1.0.0",
    "homepage": "https://github.com/damarev/filament-location-field",
    "support": {
        "issues": "https://github.com/damarev/filament-location-field/issues",
        "source": "https://github.com/damarev/filament-location-field"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dav Mat",
            "email": "damarev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0",
    },
    "require-dev": {
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "pestphp/pest-plugin-livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Damarev\\FilamentLocationField\\": "src/",
            "Damarev\\FilamentLocationField\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Damarev\\FilamentLocationField\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "lint": "pint",
        "test": "pest",
        "test:lint": "pint --test"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Damarev\\FilamentLocationField\\FilamentLocationFieldServiceProvider"
            ],
            "aliases": {
                "FilamentLocationField": "Damarev\\FilamentLocationField\\Facades\\FilamentLocationField"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
